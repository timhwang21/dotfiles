#!/bin/bash

if [[ $- == *i* ]]
then
	osascript <<EOD
		tell application "iTerm" to activate
		tell application "System Events" to tell process "iTerm" to keystroke "z ui/orion"
		tell application "System Events" to tell process "iTerm" to key code 52
		tell application "System Events" to tell process "iTerm" to keystroke "]" using command down
		tell application "System Events" to tell process "iTerm" to keystroke "z ui/orion && yarn start"
		tell application "System Events" to tell process "iTerm" to key code 52
		tell application "System Events" to tell process "iTerm" to keystroke "]" using command down
		tell application "System Events" to tell process "iTerm" to keystroke "t"
		tell application "System Events" to tell process "iTerm" to key code 52
		tell application "System Events" to tell process "iTerm" to keystroke "t" using command down
		tell application "System Events" to tell process "iTerm" to keystroke "z ui/orion && dcnginx"
		tell application "system Events" to tell process "iTerm" to key code 52
		tell application "System Events" to tell process "iTerm" to keystroke "t" using command down
		tell application "System Events" to tell process "iTerm" to keystroke "node"
		tell application "system Events" to tell process "iTerm" to key code 52
		tell application "System Events" to tell process "iTerm" to keystroke "d" using command down
		tell application "System Events" to tell process "iTerm" to keystroke "pry"
		tell application "system Events" to tell process "iTerm" to key code 52
		tell application "System Events" to tell process "iTerm" to keystroke "1" using command down
EOD
fi